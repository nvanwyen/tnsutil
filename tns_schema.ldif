# objectclasses
# on EUS servers, orclDBServer is a SUP of "orclService", not "top"
objectclasses=( 2.16.840.1.113894.2.2.1 NAME 'orclDBServer'           SUP 'top'   STRUCTURAL             MAY ( orclNetDescName $ orclVersion ) )
objectclasses=( 2.16.840.1.113894.3.2.1 NAME 'orclNetAddress'         SUP 'top'   STRUCTURAL MUST ( cn ) MAY ( orclNetAddressString $ orclNetProtocol $ orclVersion $ Description ) )
objectclasses=( 2.16.840.1.113894.3.2.8 NAME 'orclNetAddressAux1'     SUP 'top'   AUXILIARY              MAY ( orclNetHostname ) )
objectclasses=( 2.16.840.1.113894.3.2.2 NAME 'orclNetAddressList'     SUP 'top'   STRUCTURAL MUST ( cn ) MAY ( orclNetAddrList $ orclNetFailover $ orclNetLoadBalance $ orclNetSourceRoute $ orclVersion $ Description ) )
objectclasses=( 2.16.840.1.113894.3.2.3 NAME 'orclNetDescription'     SUP 'top'   STRUCTURAL MUST ( cn ) MAY ( orclNetAddrList $ orclNetInstanceName $ orclNetConnParamList $ orclNetFailover $ orclNetLoadBalance $ orclNetSdu $ orclNetServiceName $ orclNetSourceRoute $ orclSid $ orclVersion $ Description ) )
objectclasses=( 2.16.840.1.113894.3.2.7 NAME 'orclNetDescriptionAux1' SUP 'top'   AUXILIARY              MAY ( orclNetSendBufSize $ orclNetReceiveBufSize $ orclNetFailoverModeString $ orclNetInstanceRole ) )
objectclasses=( 2.16.840.1.113894.3.2.4 NAME 'orclNetDescriptionList' SUP 'top'   STRUCTURAL MUST ( cn ) MAY ( orclNetDescList $ orclVersion $ Description ) )
objectclasses=( 2.16.840.1.113894.3.2.5 NAME 'orclNetService'         SUP 'top'   STRUCTURAL MUST ( cn ) MAY ( orclNetDescName $ orclVersion $ Description ) )
objectclasses=( 2.16.840.1.113894.3.2.6 NAME 'orclNetServiceAlias'    SUP 'alias' STRUCTURAL MUST ( cn ) MAY ( orclNetDescName $ orclVersion $ Description ) ))
